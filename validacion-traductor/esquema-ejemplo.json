{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Usuario",
  "type": "object",
  "properties": {
    "nombre": {
      "type": "string"
    },
    "edad": {
      "type": "integer",
      "minimum": 0
    },
    "email": {
      "type": "string",
      "format": "email"
    }
  },
  "required": ["nombre", "email"]
}